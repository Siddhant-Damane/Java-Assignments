<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="com.webonise.hibernatexml.Student" table="Student">
		<id name="studentId" type="int" column="STUDENT_ID">
			<generator class="assigned" />
		</id>
		<property name="studentName">
			<column name="NAME" />
		</property>
		<property name="studentCgpa">
			<column name="CGPA" />
		</property>
	</class>
	<class name="com.webonise.hibernatexml.Teacher" table="Teacher">
		<id name="teacherId" type="int" column="TEACHER_ID">
			<generator class="assigned" />
		</id>
		<property name="teacherName">
			<column name="NAME" />
		</property>
		<property name="subject">
			<column name="Subject"/>
		</property>
	</class>
</hibernate-mapping>